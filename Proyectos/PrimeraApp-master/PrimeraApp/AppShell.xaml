<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PrimeraApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PrimeraApp"
    xmlns:views="clr-namespace:PrimeraApp.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="PrimeraApp">

    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate views:ListaCochePage}"
            Route="ListaCochePage" />
        <ShellContent
            Title="Lista"
            ContentTemplate="{DataTemplate views:ListaPage}"
            Route="ListaCochePage" />
    </TabBar>
    

</Shell>
